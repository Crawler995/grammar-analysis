    "format": "prettier --config .prettierrc --write ./{src,test}/**/*.{ts,tsx}",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^7.1.1",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "ts-node": "^8.9.0"
  }
